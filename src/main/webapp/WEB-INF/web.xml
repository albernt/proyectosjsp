        <url-pattern>/ListTareasServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NewTareaServlet</servlet-name>
        <servlet-class>servlet.NewTareaServlet</servlet-class>
    </servlet>
