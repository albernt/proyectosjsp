        <servlet-class>servlet.ListProyectosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListProyectosServlet</servlet-name>
        <url-pattern>/ListProyectosServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NewProyectoServlet</servlet-name>
        <servlet-class>servlet.NewProyectoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewProyectoServlet</servlet-name>
        <url-pattern>/NewProyectoServlet</url-pattern>
