        <servlet-name>NewTareaServlet</servlet-name>
        <url-pattern>/NewTareaServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteTareaServlet</servlet-name>
        <servlet-class>servlet.DeleteTareaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteTareaServlet</servlet-name>
        <url-pattern>/DeleteTareaServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteProyectoServlet</servlet-name>
        <servlet-class>servlet.DeleteProyectoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteProyectoServlet</servlet-name>
        <url-pattern>/DeleteProyectoServlet</url-pattern>
    </servlet-mapping>
</web-app>
